  cid         Int    @unique(map: "cid")
  jointsold   String @default("0") @db.VarChar(50)
  gunsold     Int    @default(0)
  acetonesold String @default("0") @db.VarChar(50)
  alcoholsold String @default("0") @db.VarChar(50)
  solventsold Int    @default(0)
