model inventory_player {
  id        Int    @id @unique(map: "id") @default(autoincrement())
  name      String @db.TinyText
  inventory String @db.LongText

  @@index([name(length: 255)], map: "name")
}

model inventory_stash {
  id        Int    @id @unique(map: "id") @default(autoincrement())
