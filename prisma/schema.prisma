}

model boosting_settings {
  /// This field was commented out because of an invalid name. Please provide a valid one that matches [a-zA-Z][a-zA-Z0-9_]*
  id Int @id @default(autoincrement()) @map("#")
  citizenid  String  @db.VarChar(255)
  background String? @db.VarChar(255)
  class      String  @default("D") @db.TinyText
  progress   Int     @default(0)
  vin        Int?

  @@index([citizenid], map: "citizenid")
}

model boostingcharacterlevels {
  boosterId       String   @id
  characterId     Int
  experienceLevel Int      @default(500)
  createdAt       DateTime @default(now())
  updatedAt       DateTime
}

model boostingcontracts {
  id               String         @id
  vehicle          String
